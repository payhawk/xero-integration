    "nodemon": "2.0.2",
    "pino-pretty": "4.0.0",
    "ts-jest": "25.3.0",
    "tslint": "6.1.1",
    "tslint-lines-between-class-members": "1.3.6",
    "typemoq": "2.1.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "jimp": "0.10.0",
    "mime-types": "2.1.26",
    "pg": "8.0.0",
    "pino": "6.0.0",
    "request": "2.88.2",
    "request-promise": "4.2.5",
    "restify": "8.5.1",
    "source-map-support": "0.5.16",
    "uuid": "7.0.3",
    "xero-node": "^3.1.2"
