  "description": "A service that Payhawk uses to integrate with Xero",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "tslint --project . && eslint --ext .ts src/ && madge -c --extensions ts --tsConfig tsconfig.json ./src && cspell '**'",
