  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/payhawk/xero-integration.git"
  },
  "author": "Payhawk team",
  "license": "MIT",
  "homepage": "https://github.com/payhawk/xero-integration#readme",
  "devDependencies": {
    "@types/jest": "25.1.4",
    "@types/mime-types": "2.1.0",
    "@types/node": "^12.12.34",
    "@types/oauth": "0.9.1",
    "@types/pg": "7.14.3",
    "@types/pino": "5.17.0",
    "@types/request-promise": "4.1.46",
    "@types/restify": "8.4.2",
    "@types/uuid": "7.0.2",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "cspell": "4.0.55",
    "eslint": "6.8.0",
    "eslint-import-resolver-typescript": "2.0.0",
    "eslint-plugin-import": "2.20.2",
    "jest": "25.2.6",
